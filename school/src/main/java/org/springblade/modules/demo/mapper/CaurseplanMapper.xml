<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.springblade.modules.demo.mapper.CaurseplanMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="caurseplanResultMap" type="org.springblade.modules.demo.entity.Caurseplan">
        <id column="id" property="id"/>
        <result column="grade_no" property="gradeNo"/>
        <result column="class_no" property="classNo"/>
        <result column="course_no" property="courseNo"/>
        <result column="teacher_no" property="teacherNo"/>
        <result column="classroom_no" property="classroomNo"/>
        <result column="class_time" property="classTime"/>
        <result column="weeks_sum" property="weeksSum"/>
        <result column="semester" property="semester"/>
        <result column="deleted" property="deleted"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>


    <select id="selectCaurseplanPage" resultMap="caurseplanResultMap">
        select * from t_caurseplan where is_deleted = 0
    </select>

</mapper>
