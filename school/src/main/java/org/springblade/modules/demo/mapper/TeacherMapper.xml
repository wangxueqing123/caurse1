<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.springblade.modules.demo.mapper.TeacherMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="teacherResultMap" type="org.springblade.modules.demo.entity.Teacher">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="cnames" property="cnames"/>
        <result column="subject" property="subject"/>
    </resultMap>


    <select id="selectTeacherPage" resultMap="teacherResultMap">
        select * from t_teacher where is_deleted = 0
    </select>

</mapper>
