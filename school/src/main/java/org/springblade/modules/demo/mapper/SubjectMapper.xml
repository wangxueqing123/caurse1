<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.springblade.modules.demo.mapper.SubjectMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="subjectResultMap" type="org.springblade.modules.demo.entity.Subject">
        <id column="id" property="id"/>
        <result column="subject" property="subject"/>
    </resultMap>


    <select id="selectSubjectPage" resultMap="subjectResultMap">
        select * from t_subject where is_deleted = 0
    </select>

</mapper>
