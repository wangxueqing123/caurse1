<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.springblade.modules.demo.mapper.StudentMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="studentResultMap" type="org.springblade.modules.demo.entity.Student">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="cname" property="cname"/>
        <result column="grade" property="grade"/>
    </resultMap>


    <select id="selectStudentPage" resultMap="studentResultMap">
        select * from t_student where is_deleted = 0
    </select>

</mapper>
